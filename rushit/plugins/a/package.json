{
  "name": "@jimp/plugin-a",
  "author": "",
  "license": "MIT",
  "version": "1.0.0",
  "description": "Utils for jimp extensions.",
  "main": "dist/index.js",
  "module": "es/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf ./es ./dist && npm run build:cjs && npm run build:esm",
    "build:esm": "tsc --module esnext --target es6 --moduleResolution node --outDir ./es",
    "build:cjs": "tsc --module commonjs --target es5 --outDir ./dist"
  },
  "dependencies": {
    "@jimp/plugin-base": "workspace:^1.0.0"
  },
  "devDependencies": {
    "@types/node": "ts4.7",
    "rimraf": "^2.7.1",
    "typescript": "~4.7.4"
  }
}