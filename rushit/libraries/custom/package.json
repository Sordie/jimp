{
  "name": "@jimp/custom",
  "version": "1.0.0",
  "description": "Interface to customize jimp configuration",
  "main": "dist/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "rimraf ./es ./dist && npm run build:cjs && npm run build:esm",
    "build:esm": "tsc --module esnext --target es6 --moduleResolution node --outDir ./es",
    "build:cjs": "tsc --module commonjs --target es5 --outDir ./dist"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "@jimp/core": "workspace:^1.0.0"
  },
  "devDependencies": {
    "@types/node": "ts4.7",
    "@types/web": "^0.0.86",
    "rimraf": "^2.7.1",
    "typescript": "~4.7.4"
  },
  "publishConfig": {
    "access": "public"
  }
}