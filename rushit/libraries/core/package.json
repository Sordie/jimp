{
  "name": "@jimp/core",
  "version": "1.0.0",
  "description": "Jimp core",
  "main": "dist/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "es",
    "index.d.ts",
    "fonts",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oliver-moran/jimp.git"
  },
  "bugs": {
    "url": "https://github.com/oliver-moran/jimp/issues"
  },
  "scripts": {
    "test": "mocha --require=ts-node/register test/**/*.js test/*.js",
    "test:watch": "npm run test -- --reporter min --watch",
    "test:coverage": "nyc npm run test",
    "build": "rimraf ./es ./dist && npm run build:cjs && npm run build:esm",
    "build:esm": "tsc --module esnext --target es6 --moduleResolution node --outDir ./es",
    "build:cjs": "tsc --module commonjs --target es5 --outDir ./dist"
  },
  "author": "Oliver Moran <oliver.moran@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "@jimp/utils": "workspace:^1.0.0",
    "any-base": "^1.1.0",
    "buffer": "^5.2.0",
    "exif-parser": "^0.1.12",
    "file-type": "^9.0.0",
    "load-bmfont": "^1.3.1",
    "mkdirp": "^0.5.1",
    "phin": "^2.9.1",
    "pixelmatch": "^4.0.2",
    "tinycolor2": "^1.4.1",
    "@jimp/test-utils": "~0.16.2"
  },
  "devDependencies": {
    "@types/node": "ts4.7",
    "@types/web": "^0.0.86",
    "rimraf": "^2.7.1",
    "typescript": "~4.7.4",
    "should": "^13.2.3",
    "ts-node": "~10.9.1",
    "mocha": "~10.2.0"
  },
  "xo": false,
  "publishConfig": {
    "access": "public"
  }
}
